msgid "Error: Failed to set switch value"
msgstr "Λάθος: Δεν ήταν δυνατή η ρύθμιση της τιμής του διακόπτη"

#~ msgid "Error: Unable to load file"
#~ msgstr "Λάθος: Δεν είναι δυνατή η φόρτωση αρχείου"

#~ msgid "Error: BAD DEVICE found"
#~ msgstr "Λάθος: Βρήκε κακή συσκευή"

#~ msgid "Error {}: Invalid parameter for the specified device."
#~ msgstr "Λάθος {}: Μη έγκυρη παράμετρος για τη συγκεκριμένη συσκευή."

#~ msgid "Error {}: Missing parameter for the specified device."
#~ msgstr "Λάθος {}: Λείπει παράμετρος για τη συγκεκριμένη συσκευή."

#~ msgid ""
#~ "Error {}: Invalid device. Supported devices: SWITCH, CLOCK, AND, NAND, "
#~ "OR, NOR, XOR, DTYPE, RC."
#~ msgstr ""
#~ "Λάθος {}: Μη έγκυρη συσκευή. Υποστηριζόμενες συσκευές: SWITCH, CLOCK, "
#~ "AND, NAND, OR, NOR, XOR, DTYPE, RC."

#~ msgid "Error {}: Specified devices does not need any parameters."
#~ msgstr ""
#~ "Λάθος {}: Οι συγκεκριμένες συσκευές δεν χρειάζονται καμία παράμετρο."

#~ msgid "Error {}: Device with such name already exists."
#~ msgstr "Λάθος {}: Η συσκευή με αυτό το όνομα υπάρχει ήδη."

#~ msgid ""
#~ "Error {}: An input is connected to an input. Must be output -> input."
#~ msgstr ""
#~ "Λάθος {}: Μια είσοδος συνδέεται με μια είσοδο. Πρέπει να είναι παραγωγή -"
#~ "> εισαγωγή."

#~ msgid ""
#~ "Error {}: An output is connected to an output. Must be output -> input."
#~ msgstr ""
#~ "Λάθος {}: Μια έξοδος συνδέεται με μια έξοδο. Πρέπει να είναι παραγωγή -> "
#~ "εισαγωγή."

#~ msgid "Error {}: Specified input is already connected."
#~ msgstr "Λάθος {}: Η καθορισμένη είσοδος είναι ήδη συνδεδεμένη."

#~ msgid "Error {}: Invalid input or output port."
#~ msgstr "Λάθος {}: Μη έγκυρη θύρα εισόδου ή εξόδου."

#~ msgid "Error {}: Device has not been defined."
#~ msgstr "Λάθος {}: Η συσκευή δεν έχει οριστεί."

#~ msgid ""
#~ "Error {}: Specified port is not an output. Only outputs can be monitored."
#~ msgstr ""
#~ "Λάθος {}: Η καθορισμένη θύρα δεν είναι έξοδος. Μόνο οι εξόδους μπορούν να "
#~ "παρακολουθούνται."

#~ msgid "Error {}: Specified port is already being monitored."
#~ msgstr "Λάθος {}: Ο καθορισμένος λιμένας παρακολουθείται ήδη."

#~ msgid "Error {}: Expected section header 'DEVICES'."
#~ msgstr "Λάθος {}: Αναμενόμενη κεφαλίδα ενότητας 'DEVICES'."

#~ msgid "Error {}: Expected section header 'CONNECTIONS'."
#~ msgstr "Λάθος {}: Αναμενόμενη κεφαλίδα ενότητας 'CONNECTIONS'."

#~ msgid ""
#~ "Error {}: There is no section header MONITOR, but a third section after "
#~ "the semicolon is found on file. Either semicolon is not placed correctly "
#~ "or section header is missing."
#~ msgstr ""
#~ "Λάθος {}: Δεν υπάρχει κεφαλίδα ενότητας MONITOR, αλλά ένα τρίτο τμήμα "
#~ "μετά το ερωτηματικό βρίσκεται στο αρχείο. Οποιοδήποτε ερωτηματικό δεν "
#~ "τοποθετείται σωστά ή λείπει η κεφαλίδα ενότητας."

#~ msgid "Error {}: Expected a colon after the section header."
#~ msgstr "Λάθος {}: Αναμενόταν ένα παχύ τελεία μετά την κεφαλίδα ενότητας."

#~ msgid "Error {}: Expected a semicolon at the end of the list."
#~ msgstr "Λάθος {}: Αναμενόταν ένα ερωτηματικό στο τέλος της λίστας."

#~ msgid ""
#~ "Error {}: Invalid device name. Name must start with a letter and must "
#~ "consist of letters and digits only. Name can't be the same as one of the "
#~ "device types."
#~ msgstr ""
#~ "Λάθος {}: Μη έγκυρο όνομα συσκευής. Το όνομα πρέπει να ξεκινά με ένα "
#~ "γράμμα και πρέπει να αποτελείται μόνο από γράμματα και ψηφία. Το όνομα "
#~ "δεν μπορεί να είναι το ίδιο με έναν από τους τύπους συσκευών."

#~ msgid ""
#~ "Error {}: A comma between list items or a semicolon at the end of the "
#~ "list expected."
#~ msgstr ""
#~ "Λάθος {}: Ένα κόμμα ανάμεσα στα στοιχεία λίστας ή ένα ερωτηματικό στο "
#~ "τέλος της λίστας αναμένεται."

#~ msgid "Error {}: After dot a valid input or output port should follow."
#~ msgstr ""
#~ "Λάθος {}: Μετά την κουκίδα πρέπει να ακολουθήσει μια έγκυρη θύρα εισόδου "
#~ "ή εξόδου."

#~ msgid "Error {}: Port must be a valid output of the specified device."
#~ msgstr ""
#~ "Λάθος {}: Η θύρα πρέπει να είναι μια έγκυρη έξοδος της συγκεκριμένης "
#~ "συσκευής."

#~ msgid "Error {}: Port must be a valid input of the specified device."
#~ msgstr ""
#~ "Λάθος {}: Η θύρα πρέπει να είναι μια έγκυρη είσοδος της συγκεκριμένης "
#~ "συσκευής."

#~ msgid "Error {}: Missing arrow in the connection description."
#~ msgstr "Λάθος {}: Λείπει το βέλος στην περιγραφή της σύνδεσης."

#~ msgid ""
#~ "Error {}: Not all inputs in the network are connected. Please check your "
#~ "circuit diagram."
#~ msgstr ""
#~ "Λάθος {}: Δεν συνδέονται όλες οι εισόδους στο δίκτυο. Ελέγξτε το "
#~ "διάγραμμα κυκλωμάτων σας."

#~ msgid "Error {}: Unexpected symbol detected."
#~ msgstr "Λάθος {}: Εμφανίστηκε απροσδόκητο σύμβολο."

#~ msgid "Error {}: File ended abruptly."
#~ msgstr "Λάθος {}: Το αρχείο έληξε απότομα."

#~ msgid ""
#~ "Error {}: A comma was found instead of a semicolon at the end of the "
#~ "list. Or a section header is missplaced. Please check the definition file."
#~ msgstr ""
#~ "Λάθος {}: Ένα κόμμα βρέθηκε αντί για ένα ερωτηματικό στο τέλος της "
#~ "λίστας. Ή μια κεφαλίδα ενότητας δεν είναι σωστή. Ελέγξτε το αρχείο "
#~ "ορισμού."

